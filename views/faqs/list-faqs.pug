extends ../layout

block content
  section#page-title(style="background-image: url('/images/headers/header-faqs.jpg'); padding: 120px 0;" )
    .container.clearfix
      h2= title
      span= subtitle
      
  section#content.custom-my-80
    .container.clearfix
      .row
        .col-md-12.mb-5
          h2 Frequently asked questions
          p A list of questions and answers relating to our organization.
      .row
          each faq in faqs
            .col-md-4
              .card
                .card-header
                  h3= faq.question
                  .card-body
                    p= faq.answer